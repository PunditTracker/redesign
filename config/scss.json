{
  "app": {
    "output_folder": "css/gen/",
    "depends": [
      "scss/*.scss"
    ],
    "rules": {
      "scss_grid": {
        "output": "grid.css",
        "inputs": [
          "scss/grid.scss"
        ]
      },
      "scss_angle": {
        "output": "angle.css",
        "inputs": [
          "scss/angle.scss"
        ]
      }
    }
  }
}